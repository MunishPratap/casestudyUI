<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>greetings</servlet-name>
  	<display-name>greetings</display-name>
  	<description></description>
  	<servlet-class>servlet.greetings</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>DbConnection</servlet-name>
  	<display-name>DbConnection</display-name>
  	<description></description>
  	<servlet-class>servlet.DbConnection</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ColumnCountValidation</servlet-name>
  	<display-name>ColumnCountValidation</display-name>
  	<description></description>
  	<servlet-class>servlet.ColumnCountValidation</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>greetings</servlet-name>
  	<url-pattern>/greetings</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>DbConnection</servlet-name>
  	<url-pattern>/DbConnection</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ColumnCountValidation</servlet-name>
  	<url-pattern>/ColumnCountValidation</url-pattern>
  </servlet-mapping>
</web-app>
